<?xml version="1.0" encoding="UTF-8"?>
<project name="OrionAnnotationsBuilder" default="all" basedir=".">
    <property name="orion.annotations.source.dir" value="${orion.annotations.dir}/${default.source.dir.name}"/>

    
    <target name="all">
    	<buildAndPackageProject sourceDir="${orion.annotations.source.dir}" binariesDir="${orion.annotations.binaries.dir}" JARPath="${orion.orionware.libraries.libraries.dir}/${orion.annotations.jar.file.name}" projectName="${orion.annotations}"/>
    </target>
	
	
	<target name="delete_JAR">
		<deleteFile path="${orion.orionware.libraries.libraries.dir}/${orion.annotations.jar.file.name}"/>
    </target>
</project>