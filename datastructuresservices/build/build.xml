<?xml version="1.0"?>
<project name="OrionDataStructuresServicesBuilder" default="all" basedir=".">
    <property file="DataStructuresServicesBuild.prop"/>
    <property name="orion.data.structures.services.binaries.dir" value="${orion.data.structures.services.dir}/${default.binaries.dir.name}"/>
    <property name="orion.data.structures.services.source.dir" value="${orion.data.structures.services.dir}/${default.source.dir.name}"/>

    
    <target name="all">
