<?xml version="1.0" encoding="UTF-8"?>
<project name="OrionDependencyInjectionBuilder" default="all" basedir=".">
    <property name="orion.dependency.injection.source.dir" value="${orion.dependency.injection.dir}/${default.source.dir.name}"/>

    
    <target name="all">
        <buildAndPackageProject sourceDir="${orion.dependency.injection.source.dir}" binariesDir="${orion.dependency.injection.binaries.dir}" JARPath="${orion.orionware.libraries.libraries.dir}/${orion.dependency.injection.jar.file.name}" projectName="${orion.dependency.injection}"/>
    </target>
</project>