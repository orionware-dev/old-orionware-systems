<?xml version="1.0" encoding="UTF-8"?>
<project name="OrionFileSystemBuilder" default="all" basedir=".">
    <property name="orion.file.system.source.dir" value="${orion.file.system.dir}/${default.source.dir.name}"/>

    
    <target name="all">
    	<buildAndPackageProject sourceDir="${orion.file.system.source.dir}" binariesDir="${orion.file.system.binaries.dir}" JARPath="${orion.orionware.libraries.libraries.dir}/${orion.file.system.jar.file.name}" projectName="${orion.file.system}"/>
    </target>
	
	
	<target name="delete_JAR">
		<deleteFile path="${orion.orionware.libraries.libraries.dir}/${orion.file.system.jar.file.name}"/>
    </target>
</project>