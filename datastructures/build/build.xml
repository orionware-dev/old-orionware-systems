<?xml version="1.0"?>
<project name="OrionDataStructuresBuilder" default="all" basedir=".">
    <property file="DataStructuresBuild.prop"/>
    <property name="orion.data.structures.binaries.dir" value="${orion.data.structures.dir}/${default.binaries.dir.name}"/>
    <property name="orion.data.structures.source.dir" value="${orion.data.structures.dir}/${default.source.dir.name}"/>

    
    <target name="all">
    	<buildProjectAndPackage sourceDir="${orion.data.structures.source.dir}" binariesDir="${orion.data.structures.binaries.dir}" JARPath="${orion.data.structures.jar.file.name}" projectName="${orion.data.structures}"/>
    </target>
</project>