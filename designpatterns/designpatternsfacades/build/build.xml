<?xml version="1.0"?>
<project name="OrionDesignPatternsFacadesBuilder" default="all" basedir=".">
    <property file="DesignPatternsFacadesBuild.prop"/>
    <property name="orion.design.patterns.facades.binaries.dir" value="${orion.design.patterns.facades.dir}/${default.binaries.dir.name}"/>
    <property name="orion.design.patterns.facades.source.dir" value="${orion.design.patterns.facades.dir}/${default.source.dir.name}"/>

    
    <target name="all">
        <buildProjectAndPackage sourceDir="${orion.design.patterns.facades.source.dir}" binariesDir="${orion.design.patterns.facades.binaries.dir}" JARPath="${orion.design.patterns.facades.jar.file.name}" projectName="${orion.design.patterns.facades}"/>
    </target>
</project>