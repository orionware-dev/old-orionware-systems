<?xml version="1.0"?>
<project name="OrionDesignPatternsBuilder" default="all" basedir=".">
    <property file="DesignPatternsBuild.prop"/>
    <property name="orion.design.patterns.binaries.dir" value="${orion.design.patterns.dir}/${default.binaries.dir.name}"/>
    <property name="orion.design.patterns.source.dir" value="${orion.design.patterns.dir}/${default.source.dir.name}"/>

    
    <target name="all">
        <buildProject sourceDir="${orion.design.patterns.source.dir}" binariesDir="${orion.design.patterns.binaries.dir}" JARPath="${orion.design.patterns.jar.file.name}" projectName="${orion.design.patterns}"/>
    </target>
</project>