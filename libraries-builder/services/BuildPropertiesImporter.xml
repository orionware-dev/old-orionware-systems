<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project name="BuildPropertiesImporter" basedir=".">
	<macrodef name="importBuildProperties">
        <sequential>
        	<property file="${builder.configuration.path}/OrionLibrariesVersion.prop"/>
            <property file="${builder.configuration.path}/OrionLibrariesDirectories.prop"/>
            <property file="${builder.configuration.path}/OrionLibrariesDefaultBuild.prop"/>
        </sequential>
    </macrodef>
    
    
    <importBuildProperties/>
</project>