<?xml version="1.0"?>
<project name="AntService" basedir=".">
    <macrodef name="initialiseAntService">
        <sequential>
            <import file="${orion.ant.tasks.macrodefs.dir}/AntCallingService.xml"/>
            <import file="${orion.ant.tasks.macrodefs.dir}/FileSystemService.xml"/>
            <import file="${orion.ant.tasks.macrodefs.dir}/JavaCompilerService.xml"/>
            <import file="${orion.ant.tasks.macrodefs.dir}/OutputService.xml"/>
            <import file="${orion.ant.tasks.macrodefs.dir}/ProjectBuilderService.xml"/>
            <taskdef resource="net/sf/antcontrib/antlib.xml" classpath="${orion.libraries.ant.dir}/ant-contrib-1.0b3.jar"/>
        </sequential>
    </macrodef>
    
    
    <initialiseAntService/>
</project>